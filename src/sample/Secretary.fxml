                        <ComboBox fx:id="lec4" prefHeight="31.0" prefWidth="169.0">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </ComboBox>
                        <ComboBox fx:id="lec5" prefHeight="31.0" prefWidth="169.0">
