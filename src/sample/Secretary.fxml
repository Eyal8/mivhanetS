                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </ComboBox>
                        <Button onAction="#selectStudent" mnemonicParsing="false" text="Select student">
                           <VBox.margin>
                              <Insets left="100.0" top="50.0" />
