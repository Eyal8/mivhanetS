                              <Insets left="40.0" top="20.0" />
                           </VBox.margin>
                        </ComboBox>
                     </children>
                  </VBox>
                  <VBox prefHeight="104.0" prefWidth="219.0">
                     <children>
                        <Label prefHeight="21.0" prefWidth="91.0" text="Course details:" />
                        <ComboBox fx:id="courseDetails" prefHeight="25.0" prefWidth="183.0" />
