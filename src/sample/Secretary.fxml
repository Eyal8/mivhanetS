                              <Insets top="5.0" />
                           </VBox.margin></ComboBox>
                        <ComboBox fx:id="tut2" prefHeight="31.0" prefWidth="169.0">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </ComboBox>
                        <ComboBox fx:id="tut3" prefHeight="31.0" prefWidth="169.0">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </ComboBox>
                        <Button mnemonicParsing="false" prefHeight="31.0" prefWidth="218.0" text="Register Employees" onAction="#getTeachingEmployees">
                           <VBox.margin>
                              <Insets top="100.0" />
                           </VBox.margin>
                           <tooltip>
                              <Tooltip text="Set the course manager, lecturers and tutors to the course" />
                           </tooltip>
